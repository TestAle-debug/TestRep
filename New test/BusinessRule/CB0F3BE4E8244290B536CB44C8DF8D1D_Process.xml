            Dim DocumentNumber as String = sp.InputVariables(&amp;quot;DOCUMENTNUMBER&amp;quot;)
            Dim DocumentType as String = sp.InputVariables(&amp;quot;DOCUMENTTYPE&amp;quot;)
            
            Dim Path as String = sp.InputVariables(&amp;quot;STORAGEPATH&amp;quot;)

