            Dim ds = New CaptureDocumentService
            Dim FileStream as Stream
            Dim fsWrite As FileStream
            
            Dim bRead As BinaryReader 
            Dim b As Byte 
            Dim bWrite As BinaryWriter
                    
            Try
            
                FileStream = ds.GetDocumentRenditionsFile(SessionID, DocumentID, &amp;quot;1&amp;quot;,&amp;quot;tiff&amp;quot;) &amp;apos; Removed PageID
                
